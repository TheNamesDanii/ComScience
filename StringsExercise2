print("Hello %s" %"world!")
print("%d + %d + %d" %(2, 2, 4))

pangram = "The quick brown fox jumps over the lazy dog!"
print(pangram[:])

word1 = "Leaving"
word2 = "Certificate"
word3 = "Computer"
word4 = "Science"
subjectName = word1 + word2 + word3 + word4
print(subjectName)

pangram = "The quick brown fox jumps over the lazy dog!"
print(pangram[:3] + pangram[16:])

print("%s" %3)
print("%d" %3.14)
print("%f" %"Hi!")
print("%f" %3)
