import turtle
turtle.shape("turtle")
turtle.color("red")
turtle.forward(50)
turtle.right(45)
turtle.color("orange")
turtle.forward(50)
turtle.right(45)
turtle.color("yellow")
turtle.forward(50)
turtle.right(45)
turtle.color("aqua")
turtle.forward(50)
turtle.right(45)
turtle.color("green")
turtle.forward(50)
turtle.right(45)
turtle.color("cyan")
turtle.forward(50)
turtle.right(45)
turtle.color("blue")
turtle.forward(50)
turtle.right(45)
turtle.color("purple")
turtle.forward(50)
turtle.exitonclick()

import turtle
for i in range(22):
    turtle.shape("turtle")
    turtle.color("red")
    turtle.forward(50)
    turtle.right(45)
    turtle.color("orange")
    turtle.forward(50)
    turtle.right(45)
    turtle.color("yellow")
    turtle.forward(50)
    turtle.right(45)
    turtle.color("aqua")
    turtle.forward(50)
    turtle.right(45)
    turtle.color("green")
    turtle.forward(50)
    turtle.right(45)
    turtle.color("cyan")
    turtle.forward(50)
    turtle.right(45)
    turtle.color("blue")
    turtle.forward(50)
    turtle.right(45)
    turtle.color("purple")
    turtle.forward(50)
    turtle.right(60)
turtle.exitonclick()

import turtle
import random
length = random.randrange(10,50,1)
sides = random.randrange(0,10,1)
angles = random.randrange(0,360,1)
turtle.shape("turtle")
for i in range(sides):
    turtle.forward(length)
    turtle.right(angles)
turtle.exitonclick()


    

 



