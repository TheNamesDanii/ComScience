import turtle
turtle.shape("turtle")
turtle.begin_fill()
turtle.color("red")
turtle.forward(50)
turtle.right(90)
turtle.forward(50)
turtle.right(90)
turtle.forward(50)
turtle.right(90)
turtle.forward(50)
turtle.penup()
turtle.right(180)
turtle.end_fill()
turtle.forward(50)
turtle.right(90)
turtle.forward(20)
turtle.right(90)
turtle.pendown()
turtle.begin_fill()
turtle.color("pink")
turtle.forward(50)
turtle.left(90)
turtle.forward(50)
turtle.left(90)
turtle.forward(50)
turtle.left(90)
turtle.forward(50)
turtle.left(180)
turtle.forward(50)
turtle.end_fill()
turtle.penup()
turtle.forward(20)
turtle.pendown()
turtle.begin_fill()
turtle.color("purple")
turtle.right(90)
turtle.forward(50)
turtle.left(90)
turtle.forward(50)
turtle.left(90)
turtle.forward(50)
turtle.left(90)
turtle.forward(50)
turtle.left(180)
turtle.forward(50)
turtle.end_fill()
turtle.exitonclick()

import turtle
turtle.shape("turtle")
turtle.right(90)
turtle.forward(50)
turtle.penup()
turtle.left(90)
turtle.forward(50)
turtle.left(90)
turtle.forward(50)
turtle.right(90)
turtle.pendown()
turtle.forward(50)
turtle.right(90)
turtle.forward(25)
turtle.right(90)
turtle.forward(50)
turtle.left(90)
turtle.forward(25)
turtle.left(90)
turtle.forward(50)
turtle.penup()
turtle.forward(50)
turtle.pendown()
turtle.forward(50)
turtle.left(90)
turtle.forward(25)
turtle.left(90)
turtle.forward(50)
turtle.left(180)
turtle.forward(50)
turtle.left(90)
turtle.forward(25)
turtle.left(90)
turtle.forward(50)
turtle.exitonclick()

