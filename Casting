marsBar = 1
cokeCan = 1.50
crispBag = 0.80
teaCup = 2
slicePan = 3.50
_5marsBar = marsBar*5
_4cokeCan = cokeCan*4
_3crispBag = crispBag*3
_2teaCup = teaCup*2
print(_5marsBar + _4cokeCan + _3crispBag + _2teaCup + slicePan)

number1 = int(input("Enter the first number: "))
number2 = input("Enter the second number: ")
number2 = int(number2)
sum = number1 + number2
print(number1, "+", number2, "=", sum)

tempF = int(input("Enter the degress in Fahrenheight. "))
tempC = ((tempF-32)*5/9)
tempC = str(round(tempC, 2))
print("The temperature in Centigrade is: ", tempC)












